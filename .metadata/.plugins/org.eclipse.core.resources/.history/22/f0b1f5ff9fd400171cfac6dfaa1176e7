package com.ascent.bean;

public class Product implements java.io.Serializable{

private Integer id; 
private String productnumber;//商品编号
private String productname;//商品名字
private String categoryno;//商品分类编号
private String title;//分类名称
private String companyname;//商品图片名称
private String companyaddress;//是否为新商品
private String city;//价格
private String job;//会员价格
private String tel;//剩余量
private String email;//库存量
private String country;//药品摘要
private String zip;//重量
private String superuser;//软删除 1.删除 2.正常

public Product(){
}

public Product(String username, String password, String fullname,String title, String companyname, String companyaddress,String city, String job, String tel, String email, String country,
String zip, String superuser) {
super();
}

public Integer getId() {
	return id;
}

public void setId(Integer id) {
	this.id = id;
}

public String getProductnumber() {
	return productnumber;
}

public void setProductnumber(String productnumber) {
	this.productnumber = productnumber;
}

public String getProductusername() {
	return productname;
}

public void setProductusername(String productusername) {
	this.productname = productusername;
}



}